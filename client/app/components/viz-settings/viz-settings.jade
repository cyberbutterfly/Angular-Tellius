div.filter-menu(ng-class="{'active' : vm.active}")
  .dialogcontent
    h4.filter-menu-title SETTINGS
    .options(ng-if="vm.viz.type != 'radialProgress' && vm.viz.type != 'bubble'")
      .relative(ng-if="vm.showSimpleCount")
        md-menu.absolute.menu-setting(md-position-mode='target-right target')
          md-button.md-icon-button(aria-label='Open demo menu', ng-click='$mdOpenMenu($event);')
            i.icon-settings
          md-menu-content(width='2')
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('sum')")
                p Sum
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('avg')")
                p Avg
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('min')")
                p Min
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('max')")
                p Max
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('median')")
                p Median
        span.axis-resolutions(ng-if="vm.showSimpleCount && vm.showYAxisResolution") ({{vm.yAxisResolution}})
        md-autocomplete.input-box(ng-if="vm.showSimpleCount"  md-floating-label='Dimensions' md-selected-item="vm.viz.yAxis.column" md-search-text="vm.searchTextSimpleCount" md-items="item in vm.xColumns | filter:searchTextSimpleCount" md-min-length="0" md-item-text="item", md-autofocus="true" md-selected-item-change="vm.simpleCountAggregation()")
          md-item-template
            span(md-highlight-text='vm.searchText', md-highlight-flags='^i') {{item}}
        
        md-input-container.md-block(ng-if="vm.showSimpleCount" flex-gt-sm='')
          label Label
          input(ng-model='vm.viz.simpleCountLabel')

      .relative(ng-if="!vm.isPolarChart && !vm.showSimpleCount")
        md-menu.absolute.menu-setting(md-position-mode='target-right target')
          md-button.md-icon-button(aria-label='Open demo menu', ng-click='vm.showXcolumnTimeAggregation ? $mdOpenMenu($event) : 0;')
            i.icon-settings
          md-menu-content(width='2')
            md-menu-item
              md-button.transparent(ng-click="vm.changeXAxisResolution('Hourly')")
                p Hourly
            md-menu-item
              md-button.transparent(ng-click="vm.changeXAxisResolution('Daily')")
                p Daily
            md-menu-item
              md-button.transparent(ng-click="vm.changeXAxisResolution('Weekly')")
                p Weekly
            md-menu-item
              md-button.transparent(ng-click="vm.changeXAxisResolution('Monthly')")
                p Monthly
            md-menu-item
              md-button.transparent(ng-click="vm.changeXAxisResolution('Yearly')")
                p Yearly
        span.axis-resolutions(ng-if="vm.showXAxisResolution" ) ({{vm.xAxisResolution}})
        md-autocomplete.input-box(ng-if="!vm.isPolarChart && !vm.showSimpleCount"  md-floating-label='Dimensions (x-axis)' md-selected-item="vm.viz.xAxis.column" md-search-text="vm.searchTextX" md-items="item in vm.xColumns | filter: vm.searchTextX" md-min-length="0" md-item-text="item",md-no-cache="false")
          md-item-template
            span(md-highlight-text='vm.searchTextX', md-highlight-flags='^i') {{item}}
      .relative(ng-if="!vm.isPolarChart && !vm.showSimpleCount")
        md-menu.absolute.menu-setting(md-position-mode='target-right target')
          md-button.md-icon-button(aria-label='Open demo menu', ng-click='$mdOpenMenu($event)')
            i.icon-settings
          md-menu-content(width='2')
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Sum')")
                p Sum
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Avg')")
                p Avg
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Min')")
                p Min
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Max')")
                p Max
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Median')")
                p Median
        span.axis-resolutions.y-axis(ng-if="vm.yAxisResolution && !vm.showSimpleCount" ) ({{vm.viz.yAxis.aggregation}})
        md-autocomplete.input-box(ng-if="(!vm.isPolarChart || vm.viz.type=='heatmap' || !vm.showSimpleCount)"  md-floating-label='Measures (y-axis)' md-selected-item="vm.viz.yAxis.column" md-search-text="vm.searchTextY" md-selected-item-change="vm.changeAxes()"  md-items="item in vm.yColumns | filter: vm.searchTextY" md-min-length="0" md-item-text="item", md-no-cache="false")
          md-item-template
            span(md-highlight-text='vm.searchTextY', md-highlight-flags='^i') {{item}}
      
      .relative(ng-if="!vm.isPolarChart && !vm.showSimpleCount && vm.viz.type=='multipleLine'")
        md-menu.absolute.menu-setting(md-position-mode='target-right target')
