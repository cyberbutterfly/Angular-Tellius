        md-autocomplete.input-box(md-floating-label='Measure (x-axis)' md-autofocus="true" md-selected-item="vm.viz.bubbleAxis" md-search-text="vm.searchBubbleAxisText" md-items="item in vm.yColumns | filter: vm.searchBubbleAxisText" md-min-length="0" md-item-text="item",  placeholder='')
          md-item-template
            span(md-highlight-text='vm.searchTextX', md-highlight-flags='^i') {{item}}

