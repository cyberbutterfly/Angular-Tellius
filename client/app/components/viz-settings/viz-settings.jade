            i.icon-settings
          md-menu-content(width='2')
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Sum')")
                p Sum
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Avg')")
                p Avg
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Min')")
                p Min
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Max')")
                p Max
            md-menu-item
              md-button.transparent(ng-click="vm.changeYAxisResolution('Median')")
                p Median
        span.axis-resolutions.y-axis(ng-if="vm.viz.lineThirdMeasure.aggregation && !vm.showSimpleCount" ) ({{vm.viz.lineThirdMeasure.aggregation}})
        md-autocomplete.input-box(ng-if="(!vm.isPolarChart || vm.viz.type=='heatmap' || !vm.showSimpleCount)"  md-floating-label='Second Measure' md-selected-item="vm.lineThirdMeasure" md-search-text="vm.searchThirdMeasureName" md-selected-item-change="vm.drawMultipleLine()"  md-items="item in vm.yColumns | filter: vm.searchThirdMeasureName" md-min-length="0" md-item-text="item", md-no-cache="false")
          md-item-template
            span(md-highlight-text='vm.searchThirdMeasureName', md-highlight-flags='^i') {{item}}
          
      .relative(ng-if="vm.isPolarChart")
        md-autocomplete.input-box(ng-if="vm.isPolarChart", md-floating-label='Measure 1' md-selected-item="vm.viz.polarArea[0]" md-search-text="vm.searchFirstPolarMeasure" md-items="item in vm.yColumns | filter: vm.searchFirstPolarMeasure" md-min-length="0" md-item-text="item", )
          md-item-template
            span(md-highlight-text='vm.searchFirstPolarMeasure', md-highlight-flags='^i') {{item}}

      
      .relative(ng-if="vm.isPolarChart && vm.yAxis")
        md-autocomplete.input-box(ng-if="vm.isPolarChart", md-floating-label='Measure 2' md-selected-item="vm.viz.polarArea[1]" md-search-text="vm.searchSecondPolarMeasure" md-items="item in vm.yColumns | filter: vm.searchSecondPolarMeasure" md-min-length="0" md-item-text="item", md-no-cache="false")
          md-item-template
            span(md-highlight-text='vm.searchSecondPolarMeasure', md-highlight-flags='^i') {{item}}

      
      .relative(ng-if="vm.isPolarChart && vm.viz.polarArea[1]")
        md-autocomplete.input-box(ng-if="vm.isPolarChart", md-floating-label='Measure 3' md-selected-item="vm.viz.polarArea[2]" md-search-text="vm.searchThirdPolarMeasure" md-items="item in vm.yColumns | filter: vm.searchThirdPolarMeasure" md-min-length="0" md-item-text="item", md-no-cache="false")
          md-item-template
            span(md-highlight-text='vm.searchThirdPolarMeasure', md-highlight-flags='^i') {{item}}
      
      .relative(ng-if="vm.viz.type == 'heatmap'")
        md-autocomplete.input-box(ng-if="vm.viz.type == 'heatmap'", md-floating-label='Dimension' md-selected-item="vm.viz.color" md-search-text="vm.searchHeatmapDimension" md-selected-item-change="vm.addHeatmapDimension()" md-items="item in vm.xColumns | filter: vm.searchHeatmapDimension" md-min-length="0" md-item-text="item", md-no-cache="false")
          md-item-template
            span(md-highlight-text='vm.thirdMeasure', md-highlight-flags='^i') {{item}}

      .relative(ng-if="vm.showStacked && (vm.viz.type == 'barsHorizontalStacked' || vm.viz.type == 'stackedArea' || vm.viz.type == 'barsVerticalStacked' || vm.viz.type == 'bar' || vm.viz.type == 'column' || vm.viz.type == 'area' || vm.viz.type == 'combineBarStacked' )")
        md-autocomplete.input-box(md-floating-label='Stack by' md-selected-item="vm.viz.color" md-search-text="vm.searchStackBy"  md-items="item in vm.xColumns | filter: vm.searchStackBy" md-min-length="0" md-selected-item-change="vm.stackBy()" md-item-text="item", md-no-cache="false")
          md-item-template
            span(md-highlight-text='vm.thirdMeasure', md-highlight-flags='^i') {{item}}
      .relative(ng-if="vm.viz.type == 'combineBarChart' || vm.viz.type == 'combineColChart' || vm.viz.type == 'combineBarStacked'")
        md-autocomplete.input-box(md-search-text="vm.newYColumn" md-items='item in vm.yColumns | filter: vm.newYColumn', md-item-text='item', md-min-length='0',md-selected-item="vm.selectedYColumn", md-floating-label='Combine With (y-axis)' md-selected-item-change="vm.compareWith()")
          md-item-template
            span(md-highlight-text='vm.newYColumn', md-highlight-flags='^i') {{item}}
      div
        label.setting-label.color-palette-label Viz Colors
        .relative.color-palette
          md-select.color-palette-select.color-palett.min-width-200(placeholder='Color Palette', md-container-class='modalSelect hovered vizpad-dialog-wrapper', ng-model='user.graphColors1', md-on-open='')
            md-option(selected="{{palettes.id == vm.viz.colorPaletteDataColors.id ? 'selected' : '' }}" ,value='{{palettes}}' ng-repeat="palettes in vm.colorPalette", ng-click="vm.colorPaletteSelected(palettes, $index)")
              span.color-palette-box
                span.single-color-box(ng-repeat="colors in palettes.colors", style="background: {{colors}};" ng-show="$index < 5")
      div
        label.setting-label.color-palette-label Theme
        .relative.color-palette
          md-select.color-palette-select.theme-select.min-width-200(placeholder='Color Palette', md-container-class='modalSelect hovered vizpad-dialog-wrapper', ng-model='user.themeColors1', md-on-open='')
            md-option(ng-model='user.themeColors1',value='Dark' , ng-click="vm.themeColorPaletteSelected($event,  'dark')") Dark
            md-option(ng-model='user.themeColors1',value='Sand' , ng-click="vm.themeColorPaletteSelected($event,  'sand')") Sand
            md-option(ng-model='user.themeColors1',value='Default' , ng-click="vm.themeColorPaletteSelected($event,  'default')" selected="true") Default
