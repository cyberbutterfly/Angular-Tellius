        span.axis-resolutions(ng-if="vm.viz.yAxis.aggregation != ''" ) ({{vm.viz.yAxis.aggregation}})
        md-autocomplete.input-box(md-floating-label='Measures (y-axis)' md-autofocus="true" md-selected-item="vm.viz.yAxis.column" md-search-text="vm.searchTextY" md-items="item in vm.yColumns | filter: vm.searchTextY" md-min-length="0" md-item-text="item",  placeholder='')
          md-item-template
            span(md-highlight-text='vm.searchTextY', md-highlight-flags='^i') {{item}}
        
