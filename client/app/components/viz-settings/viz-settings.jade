        md-autocomplete.input-box(md-floating-label='Measure(Radius)' md-selected-item="vm.viz.radius" md-search-text="vm.searchThirdMeasure"  md-items="item in vm.yColumns | filter: vm.searchThirdMeasure" md-selected-item-change="vm.addFourthMeasure()" md-min-length="0" md-item-text="item", md-no-cache="false")
          md-item-template
